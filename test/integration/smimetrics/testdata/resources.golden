    "namespace": "{{.Ns}}",
    "selfLink": "\/apis\/metrics\.smi-spec\.io\/v1alpha1\/namespaces\/{{.Ns}}\/deployments\/linkerd-controller",
    "creationTimestamp": "\d+-\d+-\d+T\d+:\d+:\d+Z"
  \},
  "timestamp": "\d+-\d+-\d+T\d+:\d+:\d+Z",
  "window": "\d+s",
  "resource": \{
